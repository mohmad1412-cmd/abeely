# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google OAuth Configuration (for popup sign-in)
# Get this from Google Cloud Console: https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com

# AI Configuration
# Anthropic Claude (Primary)
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here
# OpenAI GPT (Alternative/Backup - used in round-robin with Anthropic)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Twilio Verify Configuration
VITE_TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
VITE_TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
VITE_TWILIO_VERIFY_SERVICE_SID=VAxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Google Maps API (for city autocomplete)
# Get this from Google Cloud Console: https://console.cloud.google.com/apis/library/places-backend.googleapis.com
# Enable "Places API" and "Maps JavaScript API"
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
# Google Maps Map ID (required for Advanced Markers)
# Create a Map ID in Google Cloud Console: https://console.cloud.google.com/google/maps-apis/studio/maps
# Then add it here (e.g., "abc123def456")
VITE_GOOGLE_MAPS_MAP_ID=your_google_maps_map_id_here

